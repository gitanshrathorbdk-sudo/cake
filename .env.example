# Firebase Public Config (Safe to be public)
# Copy this file to .env and fill in your values from the Firebase Console
# Go to Project Settings > General > Your apps > Firebase SDK snippet > Config
NEXT_PUBLIC_FIREBASE_PROJECT_ID="your-project-id"
NEXT_PUBLIC_FIREBASE_APP_ID="your-app-id"
NEXT_PUBLIC_FIREBASE_API_KEY="your-web-api-key"
NEXT_PUBLIC_FIREBASE_AUTH_DOMAIN="your-auth-domain"
NEXT_PUBLIC_FIREBASE_MESSAGING_SENDER_ID="your-sender-id"

# Gemini API Key (MUST BE KEPT SECRET)
# Do not prefix with NEXT_PUBLIC_
GEMINI_API_KEY="your-gemini-api-key"
